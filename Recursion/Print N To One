package Recursion;
import java.util.*;
public class printNtoOne {
    public static void print(int n ){
        System.out.println(n);
        if(n==1) return ;
        print(n-1);
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the Number : ");
        int n = sc.nextInt();
        print(n);
    }
}

package strings;

public class printString {
    // WAP to print all the subSting in java;
    public static void main(String[] args) {
        String str = "Aman";
        for (int i = 0; i <= str.length(); i++) {
            for (int j = i+1; j <str.length() ; j++) {
                System.out.print(str.substring(i,j)+" ");
            }
            System.out.println();
        }
    }
}

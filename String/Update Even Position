package strings;

public class updateEvenPosition {
    // WAP to Upadte the String for Even Position
    public static void main(String[] args) {
        String s = "Raghav";
        String str = " ";
        for (int i = 0; i <s.length() ; i++) {
            if(i%2==0) str+='a';
            else str+= s.charAt(i);

        }
        System.out.println(str);
    }
}

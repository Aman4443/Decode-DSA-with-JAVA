package PatternPrinting;
import java.util.*;
public class samecount {
    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
        System.out.print("Enter the Value : ");
        int n = sc.nextInt();
        for (int i = 1; i <=n; i++) { //rows
            for (int j = 1; j <=n ; j++) { // rows
                System.out.print(i+" ");
            }
            System.out.println();
        }
    }
}

